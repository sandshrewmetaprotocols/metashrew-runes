{
  "version": "0.1.0",
  "main": "lib/index.js",
  "scripts": {
    "asbuild:debug": "asc assembly/index.ts --target debug --enable threads --config asconfig.json",
    "asbuild:release": "asc assembly/index.ts --target release --config asconfig.json",
    "build": "npm-run-all build:protoc asbuild:debug asbuild:release build:ts",
    "build:debug:preloader": "npm run asbuild:preloader --sharedMemory",
    "build:ts": "tsc",
    "build:protoc:as": "protoc --plugin=./node_modules/.bin/protoc-gen-as --as_out=assembly/proto --as_opt targetFileName=metashrew-runes.ts --proto_path=./ proto/metashrew-runes.proto",
    "build:protoc:ts": "protoc --ts_out src.ts/proto --proto_path proto metashrew-runes.proto",
    "build:protoc": "npm-run-all build:protoc:as build:protoc:ts",
    "prettier:assemblyscript": "prettier --write ./assembly/**/*.ts",
    "prettier:ts": "prettier --write ./src.ts/**/*.ts ./tests/**.ts ./tests/**/*.ts",
    "prettier": "npm-run-all prettier:assemblyscript prettier:ts",
    "test": "ts-mocha --timeout 0 ./tests/**.spec.ts",
    "test:view": "ts-mocha --timeout 0 ./tests/view.spec.ts",
    "test:release": "ts-mocha --timeout 0 ./tests/release.spec.ts",
    "clean": "rm -rf build lib assembly/proto/metashrew-runes.ts src.ts/proto/metashrew-runes.ts"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.9.4",
    "safe-buffer": "^5.2.1"
  },
  "devDependencies": {
    "@bitauth/libauth": "^3.0.0",
    "@magiceden-oss/runestone-lib": "^1.0.2",
    "@protobuf-ts/protoc": "^2.9.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "as-bignum": "^0.3.0",
    "assemblyscript": "^0.27.22",
    "assemblyscript-prettier": "^3.0.1",
    "bitcoinjs-lib": "^6.1.6",
    "chai": "4.0",
    "fs-extra": "^11.2.0",
    "js-sha3": "^0.9.3",
    "metashrew-as": "https://github.com/sandshrewmetaprotocols/metashrew-as#new-encoding",
    "metashrew-spendables": "https://github.com/sandshrewmetaprotocols/metashrew-spendables",
    "metashrew-test": "https://github.com/sandshrewmetaprotocols/metashrew-test",
    "mocha": "^10.5.2",
    "nice-repl": "^0.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "protobuf-as": "https://github.com/sandshrewmetaprotocols/protobuf-as",
    "rlp": "^3.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "name": "metashrew-runes",
  "description": "metashrew-runes"
}
